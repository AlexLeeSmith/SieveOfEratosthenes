    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serial Sieve",
            "preLaunchTask": "Build Sieve",
            "postDebugTask": "Clean Bin/Obj",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Bin/serial_prime_driver",
            "args": ["1000000", "RECURSIVE"],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "OMP Sieve",
            "preLaunchTask": "Build Sieve",
            "postDebugTask": "Clean Bin/Obj",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Bin/omp_prime_driver",
            "args": ["2", "1000000", "ROMP"],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}